{
  "name": "qrcode-room",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register --project tsconfig.server.json server.ts",
    "build": "next build",
    "build:server": "tsc --project tsconfig.server.json && cp -r .next/static .next/standalone/.next/static && cp -r public .next/standalone/ && cp -r prisma .next/standalone && cp Dockerfile .next/standalone",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "db:seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@prisma/client": "^6.5.0",
    "date-fns": "^4.1.0",
    "jimp": "^1.6.0",
    "jose": "^6.0.10",
    "jsqr": "^1.4.0",
    "module-alias": "^2.2.3",
    "next": "15.2.3",
    "prisma": "^6.5.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-vant": "^3.3.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.17.25",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
